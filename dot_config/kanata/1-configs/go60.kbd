(defcfg
  ;; Go60 has no product_key so `kanata --list` shows only its hash.
  ;; But Karabiner DriverKit changes hashes, while preserving device names.
  ;; The name "Go60" is visible at the IOKit level, so match by name.
  macos-dev-names-include (
    "Go60"
  )

  process-unmapped-keys yes
  log-layer-changes no
)

(include shared/common.kbd)

(defsrc)

(deflayermap (base)
  tab @tab
  rmet @wm-modifier
  lctl @lsecondary
)

(deflayermap (fn))
