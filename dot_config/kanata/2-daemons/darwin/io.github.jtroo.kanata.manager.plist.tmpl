<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>io.github.jtroo.kanata.manager</string>

  <key>ProgramArguments</key>
  <array>
    <string>{{ .chezmoi.destDir }}/.config/kanata/2-daemons/darwin/kanata-manager.sh</string>
  </array>

  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <true/>

  <key>StandardOutPath</key>
  <string>/tmp/io.github.jtroo.kanata.manager.log</string>
  <key>StandardErrorPath</key>
  <string>/tmp/io.github.jtroo.kanata.manager.log</string>
</dict>
</plist>
